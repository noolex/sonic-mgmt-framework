<CLISH_MODULE
    xmlns="http://www.dellemc.com/sonic/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema
    http://www.dellemc.com/sonic/XMLSchema/clish.xsd"
    >
  <VIEW name="enable-view">
    <COMMAND name="clear queue" help="Clear Queue information">
         <PARAM
             name="queue-subcommands"
             help="queue subcommands"
             mode="switch"
             ptype="SUBCOMMAND"
             >
             <!-- clear queue counters -->
             <PARAM
                 name="counters"
                 help="Counters for all interface, all queues"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
                 <!-- clear queue counters interface Ethernet [<id>] -->
                 <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                 <PARAM
                     name="Ethernet"
                     help="Counters for interface queues"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <PARAM
                        name="phy-if-id"
                        help="Physical interface"
                        ptype="UINT"
                        >
                        <!-- clear queue counters interface Ethernet [<id>] queue [<queue-name>] -->
                        <PARAM
                           name="queue"
                           help="Counters for interface queue"
                           ptype="SUBCOMMAND"
                           mode="subcommand"
                           optional="true"
                           >
                           <PARAM
                              name="queue-id"
                              help="Queue id: 0, 1, .. etc"
                              ptype="STRING"
                              >
                           </PARAM>
                        </PARAM>
                     </PARAM>
                 </PARAM>
                 </PARAM>
             </PARAM>
             <!-- clear queue watermark -->
             <PARAM
                 name="watermark"
                 help="Watermark for all interface queues"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
               <PARAM
                 name="queue-type-subcommands"
                 help="queue type subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND"
                 >
                 <!-- clear queue watermark unicast --> 
                 <PARAM
                     name="unicast"
                     help="Watermark for all interface unicast queues"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear queue watermark unicast interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Watermark for interface unicast queues"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
                 <!-- clear queue watermark multicast --> 
                 <PARAM
                     name="multicast"
                     help="Watermark for all interface multicast queues"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear queue watermark multicast interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Watermark for interface multicast queues"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
               </PARAM>
             </PARAM>
             <!-- clear queue persistent-watermark -->
             <PARAM
                 name="persistent-watermark"
                 help="Persistent-Watermark for all interface queues"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
               <PARAM
                 name="queue-type-subcommands"
                 help="queue type subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND"
                 >
                 <!-- clear queue persistent-watermark unicast --> 
                 <PARAM
                     name="unicast"
                     help="Persistent-Watermark for all interface unicast queues"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear queue persistent-watermark unicast Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Persistent-Watermark for interface unicast queues"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
                 <!-- clear queue persistent-watermark multicast --> 
                 <PARAM
                     name="multicast"
                     help="Persistent-Watermark for all interface multicast queues"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear queue persistent-watermark multicast interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Persistent-Watermark for interface multicast queues"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         optional="true"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
               </PARAM>
             </PARAM>
             <!-- clear queue buffer-threshold-breaches -->
             <PARAM
                 name="buffer-threshold-breaches"
                 help="buffer threshold breaches"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
             </PARAM>
         </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_clear_qos clear_qos ${__full_line}</ACTION>
      <xi:include href="include/pipe.xml"/>
    </COMMAND>
    <COMMAND name="clear priority-group" help="Clear priority group counters">
         <PARAM
            name="priority-group-subcommands"
            help="priority-group subcommands"
            mode="switch"
            ptype="SUBCOMMAND"
            >
             <!-- clear priority-group watermark -->
             <PARAM
                 name="watermark"
                 help="Watermark for all interface priority-groups"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
               <PARAM
                 name="priority-group-watermark-subcommands"
                 help="priority group watermark subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND"
                 >
                 <!-- clear priority-group watermark headroom --> 
                 <PARAM
                     name="headroom"
                     help="Watermark in the headroom for all interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear priority-group watermark headroom interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Watermark in the headroom for interface "
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
                 <!-- clear priority-group watermark shared --> 
                 <PARAM
                     name="shared"
                     help="Watermark in shared buffer for all interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear priority-group watermark shared interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Watermark in shared buffer for interface"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
               </PARAM>
             </PARAM>
             <!-- clear priority-group persistent-watermark -->
             <PARAM
                 name="persistent-watermark"
                 help="Persistent-Watermark for all interface "
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
               <PARAM
                 name="priority-group-watermark-subcommands"
                 help="priority-group watermark subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND"
                 >
                 <!-- clear priority-group persistent-watermark headroom --> 
                 <PARAM
                     name="headroom"
                     help="Persistent-Watermark in the headroom for all interface "
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear priority-group persistent-watermark headroom interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Persistent-Watermark in the headroom for interface "
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
                 <!-- clear priority-group persistent-watermark shared --> 
                 <PARAM
                     name="shared"
                     help="Persistent-Watermark in shared buffer for all interface "
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     >
                     <!-- clear priority-group persistent-watermark shared interface Ethernet [<id>] -->
                     <PARAM 
                     name="interface"
                     help="Clear counters interface"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                     <PARAM
                         name="Ethernet"
                         help="Persistent-Watermark in shared buffer for interface "
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                         <PARAM
                             name="phy-if-id"
                             help="Physical interface"
                             ptype="UINT"
                             >
                         </PARAM>
                     </PARAM>
                     </PARAM>
                 </PARAM>
               </PARAM>
             </PARAM>
         </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_clear_qos clear_qos ${__full_line}</ACTION>
      <xi:include href="include/pipe.xml"/>
    </COMMAND>
  </VIEW>
 </CLISH_MODULE>
