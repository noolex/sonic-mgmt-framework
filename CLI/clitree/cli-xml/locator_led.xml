<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema http://www.dellemc.com/sonic/XMLSchema/clish.xsd">
  <!--=========================================================================-->
  <PTYPE name="LOGGING_COUNT" method="integer" pattern="1..65535" help="port"/>
  <!--=======================================================-->

  <VIEW name="enable-view">
    <!-- locator-led  -->
    <COMMAND
         name="show locator-led"
         help="Display Locator LED State"
         >
        <PARAM name="chassis"
               help="Display Locator Chassis LED State"  
               mode="subcommand"
               ptype="SUBCOMMAND">
         </PARAM>
       <ACTION
           builtin="clish_pyobj">sonic_cli_locator_led show_locator_led_chassis show_locator_led.j2
       </ACTION>
    </COMMAND>

    <COMMAND
         name="locator-led" help="Locator Chassis LED Utility">
    </COMMAND>
    <COMMAND 
        name="locator-led chassis" help="Locator Chassis LED Operation">
    </COMMAND>

    <COMMAND name="locator-led chassis on" help="Enable Locator Chassis LED"> 
       <ACTION builtin="clish_pyobj">sonic_cli_locator_led locator_led_chassis_on show_locator_led.j2 </ACTION>
    </COMMAND>
    <COMMAND name="locator-led chassis off" help="Disable Locator Chassis LED"> 
       <ACTION builtin="clish_pyobj">sonic_cli_locator_led locator_led_chassis_off show_locator_led.j2 </ACTION>
    </COMMAND>
  </VIEW>

</CLISH_MODULE>
