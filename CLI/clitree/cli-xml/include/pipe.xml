<!-- 1st pipe -->
<PARAM xmlns="http://www.dellemc.com/sonic/XMLSchema"
    name="1_pipe"
    value="|"
    help="Pipe through a command"
    ptype="SUBCOMMAND"
    order="true"
    optional="true"
    mode="subcommand">

    <PARAM name="1_switch_var"
        help="pipe switch_var"
        ptype="STRING"
        mode="switch">

        <PARAM name="except"
            help="Show only text that does not match a pattern"
            ptype="SUBCOMMAND"
            mode="subcommand">
            <PARAM name="1_LINE"
            help="Regular expression"
            ptype="STRING_WITH_PIPE">
            </PARAM>

            <PARAM name="1_ignore-case"
                value="ignore-case"
                help="Case insensitive"
                ptype="SUBCOMMAND"
                mode="subcommand"
                optional="true">
            </PARAM>
        </PARAM>

        <PARAM name="find"
            help="Search for the 1st occurrence of a pattern and display all the subsequent configs"
            ptype="SUBCOMMAND"
            mode="subcommand">
            <PARAM name="1_LINE"
                help="Regular expression"
                ptype="STRING_WITH_PIPE">
            </PARAM>

            <PARAM name="1_ignore-case"
                value="ignore-case"
                help="Case insensitive"
                ptype="SUBCOMMAND"
                mode="subcommand"
                optional="true">
            </PARAM>
        </PARAM>

        <PARAM name="grep"
            help="Show only text that matches a pattern"
                ptype="SUBCOMMAND"
                mode="subcommand">
                <PARAM name="1_LINE"
                    help="Regular expression"
                    ptype="STRING_WITH_PIPE">
                </PARAM>

            <PARAM name="1_ignore-case"
                value="ignore-case"
                help="Case insensitive"
                ptype="SUBCOMMAND"
                mode="subcommand"
                optional="true">
            </PARAM>
        </PARAM>

        <PARAM name="no-more"
            help="Don't paginate output"
            ptype="SUBCOMMAND"
            mode="subcommand">
        </PARAM>

        <PARAM name="save"
            help="Save output to a file"
            ptype="SUBCOMMAND"
            optional="true"
            mode="subcommand">
            <PARAM name="filename"
                help="Save to the file"
                ptype="STRING">
                <PARAM name="append"
                    help="Append output"
                    optional="true"
                    ptype="SUBCOMMAND"
                    mode="subcommand"/>
            </PARAM>
        </PARAM>
    </PARAM>
<!-- end of 1st pipe -->
