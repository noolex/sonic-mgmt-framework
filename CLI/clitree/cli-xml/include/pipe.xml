                </PARAM>
            </PARAM>

            <PARAM name="no-more"
                help="Don't paginate output"
                ptype="SUBCOMMAND"
                mode="subcommand">
            </PARAM>

            <PARAM name="save"
                help="Save output to a file"
                ptype="SUBCOMMAND"
                optional="true"
                mode="subcommand">
                <PARAM name="filename"
                help="Save to the file"
                ptype="STRING">
                <PARAM name="append"
                    help="Append output"
                    optional="true"
                    ptype="SUBCOMMAND"
                    mode="subcommand"/>
                </PARAM>
            </PARAM>
        </PARAM>
    </PARAM>
