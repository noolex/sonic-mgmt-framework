                    if test "${queue}" != ""; then&#xA;
                        python $SONIC_CLI_ROOT/sonic_cli_qos_pfc.py  show_port_pfc_queue_statistics  ${phy-intf-name}  show_qos_interface_pfc_queue_statistics.j2&#xA;
