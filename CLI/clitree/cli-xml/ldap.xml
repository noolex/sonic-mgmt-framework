<!--=========================================================================-->

<PTYPE
     name="LDAP_SEARCH_TIME_LIMIT"
     method="integer"
     pattern="1..65535"
     help=""
     />
     
<PTYPE
     name="LDAP_BIND_TIME_LIMIT"
     method="integer"
     pattern="1..65535"
     help=""
     />

<PTYPE
     name="LDAP_IDLE_TIME_LIMIT"
     method="integer"
     pattern="1..65535"
     help=""
     />

<PTYPE
     name="RETRY_ATTEMPTS"
     method="integer"
     pattern="0..10"
     help=""
     />

<PTYPE
     name="LDAP_PORT"
     method="integer"
     pattern="1..65535"
     help=""
     />

<PTYPE
     name="LDAP_SCOPE"
     method="select"
     pattern="sub(sub) one(one) base(base)"
     help=""
     />

<PTYPE
     name="LDAP_VERSION"
     method="integer"
     pattern="2..3"
     help=""
     />

<PTYPE
     name="LDAP_SSL"
     method="select"
     pattern="on(on) off(off) start_tls(start_tls)"
     help=""
     />

<PTYPE
     name="LDAP_USE_TYPE"
     method="select"
     pattern="all(all) nss(nss) sudo(sudo) pam(pam)"
     help=""
     />
 
    
<PTYPE
     name="LDAP_SERVER_PORT"
     method="integer"
     pattern="1..65535"
     help=""
     />

<PTYPE
     name="LDAP_SERVER_PRIORITY"
     method="integer"
     pattern="1..99"
     help=""
     />
     
<PTYPE
     name="LDAP_VRF"
     pattern="(Vrf(([0-9A-Za-z]){1,28}))|(mgmt)"
     help="Name of VRF (Max size 32, (mgmt, or prefixed by Vrf)"
/>

