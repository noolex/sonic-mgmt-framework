<CLISH_MODULE
    xmlns="http://www.dellemc.com/sonic/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema
    http://www.dellemc.com/sonic/XMLSchema/clish.xsd"
    >
    <PTYPE
    name="TYPE"
    method="select"
    pattern="snat dnat"
    help="NAT type"/>

    <PTYPE
    name="PORT_TYPE"
    method="select"
    pattern="tcp udp"
    help="NAT Port type"/>

   <VIEW
       name="configure-view"
       >
    <!-- nat commands -->
    <COMMAND
        name="nat"
	view="configure-nat-view"
        help="Enter NAT global configuration"
        />

   </VIEW>

   <VIEW
        name="configure-nat-view"
        prompt="${SYSTEM_NAME}(config-nat)# "
        depth="2"
        >
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
            />
        <COMMAND
            name="no"
            help="Negate a command or set its defaults"
	    />
        <COMMAND
            name="enable"
            help="Enable NAT feature"
	    >
        <ACTION builtin="clish_pyobj">sonic_cli_nat patch_openconfig_nat_nat_instances_instance_config_enable True</ACTION>
        </COMMAND>

	<COMMAND
            name="no enable"
            help="Disable NAT feature"
            >
        <ACTION builtin="clish_pyobj">sonic_cli_nat patch_openconfig_nat_nat_instances_instance_config_enable False</ACTION>
       </COMMAND>

       <COMMAND
         name="timeout"
         help="Configure NAT entry aging timeout in seconds"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="timeout-value"
             help="NAT Timeout"
             ptype="NAT_TIMEOUT"
             >
         </PARAM>
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/timeout body={"openconfig-nat:timeout": ${timeout-value}}</ACTION>
       </COMMAND>


       <COMMAND
         name="no timeout"
         help="Set default NAT entry aging timeout in seconds"
	 >
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/timeout body={"openconfig-nat:timeout": 600}</ACTION>
     </COMMAND>

     <COMMAND
         name="tcp-timeout"
         help="Configure TCP NAT entry aging timeout in seconds"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="tcp-timeout-value"
             help="TCP NAT Timeout"
             ptype="NAT_TCP_TIMEOUT"
             >
         </PARAM>
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/tcp-timeout body={"openconfig-nat:tcp-timeout": ${tcp-timeout-value}}</ACTION>
     </COMMAND>

     <COMMAND
         name="no tcp-timeout"
         help="Set Default TCP NAT entry aging timeout in seconds"
         >
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/tcp-timeout body={"openconfig-nat:tcp-timeout": 86400}</ACTION>
     </COMMAND>

      <COMMAND
         name="udp-timeout"
         help="Configure UDP NAT entry aging timeout in seconds"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="udp-timeout-value"
             help="UDP NAT Timeout"
             ptype="NAT_UDP_TIMEOUT"
             >
         </PARAM>
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/udp-timeout body={"openconfig-nat:udp-timeout": ${udp-timeout-value}}</ACTION>
     </COMMAND>

      <COMMAND
         name="no udp-timeout"
         help="Set Default UDP NAT entry aging timeout in seconds"
         >
         <ACTION builtin="clish_restcl">oper=PATCH url=/restconf/data/openconfig-nat:nat/instances/instance=0/config/udp-timeout body={"openconfig-nat:udp-timeout": 300}</ACTION>
     </COMMAND>

      <COMMAND
         name="pool"
         help="Create NAT Pool"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="pool-name"
             help="NAT Pool name"
             ptype="STRING"
             >
         </PARAM>
         <PARAM
             name="global-ip-range"
             help="Global IP range"
             ptype="STRING"
             >
         </PARAM>
         <PARAM
             name="global-port-range"
             help="Global Port Range"
             ptype="STRING"
             optional="true"
             >
         </PARAM>

     <ACTION builtin="clish_pyobj">sonic_cli_nat patch_openconfig_nat_nat_instances_instance_nat_pool_nat_pool_entry_config ${pool-name} ${global-ip-range} ${global-port-range}</ACTION>
     </COMMAND>


     <COMMAND
         name="no pool"
         help="Remove NAT Pool"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="pool-name"
             help="NAT Pool name"
             ptype="STRING"
             >
         </PARAM>
         <ACTION builtin="clish_restcl">oper=DELETE url=/restconf/data/openconfig-nat:nat/instances/instance=0/nat-pool/nat-pool-entry=${pool-name}</ACTION>
     </COMMAND>

     <COMMAND
         name="no pools"
         help="Remove all NAT Pools"
	 >
     <ACTION builtin="clish_restcl">oper=DELETE url=/restconf/data/openconfig-nat:nat/instances/instance=0/nat-pool</ACTION>
     </COMMAND>

     <COMMAND
         name="binding"
         help="Create binding between an ACL and a NAT pool"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
	 <PARAM
             name="binding-name"
             help="NAT binding name"
             ptype="STRING"
             >
         </PARAM>
         <PARAM
             name="pool-name"
             help="NAT pool name"
             ptype="STRING"
             >
         </PARAM>
         <PARAM
             name="acl-name"
             help="ACL Name"
             ptype="STRING"
             optional="true"
             >
         </PARAM>
         <PARAM
             name="natType"
             help=""
             ptype="TYPE"
             optional="true"
             >
         </PARAM>
         <PARAM
             name="twice-nat-id"
             help="Twice NAT ID"
             ptype="SUBCOMMAND"
             mode="subcommand"
             optional="true"
             >
             <PARAM
                 name="twice-nat-id-value"
                 help="Twice NAT ID value"
                 ptype="UINT"
                 >
             </PARAM>
         </PARAM>

         <ACTION builtin="clish_pyobj">sonic_cli_nat patch_openconfig_nat_nat_instances_instance_nat_acl_pool_binding_nat_acl_pool_binding_entry_config ${binding-name} ${pool-name} acl-name=${acl-name} nat-type=${natType} twice-nat-id=${twice-nat-id-value}</ACTION>
     </COMMAND>

      <COMMAND
         name="no binding"
         help="Remove NAT binding"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="bind-name"
             help="NAT binding name"
             ptype="STRING"
             >
         </PARAM>
         <ACTION builtin="clish_restcl">oper=DELETE url=/restconf/data/openconfig-nat:nat/instances/instance=0/nat-acl-pool-binding/nat-acl-pool-binding-entry=${bind-name}/config</ACTION>
     </COMMAND>

     <COMMAND
         name="no bindings"
         help="Remove all NAT bindings"
     >
     <ACTION builtin="clish_restcl">oper=DELETE url=/restconf/data/openconfig-nat:nat/instances/instance=0/nat-acl-pool-binding</ACTION>
     </COMMAND>

     <COMMAND
         name="static"
         help="Add static NAT entry"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="static-subcommands"
             help="Static NAT subcommands"
             mode="switch"
             ptype="SUBCOMMAND"
             >
         <PARAM
             name="basic"
             help="Static NAT"
             ptype="SUBCOMMAND"
             mode="subcommand"
             >

             <PARAM
                 name="global-ip"
                 help="Global IP"
                 ptype="IP_ADDR"
                 >
             </PARAM>

             <PARAM
                 name="local-ip"
                 help="Local IP"
                 ptype="IP_ADDR"
                 >
             </PARAM>

             <PARAM
                 name="natType"
                 help=""
                 ptype="TYPE"
                 optional="true"
                 >
             </PARAM>

             <PARAM
                 name="twice-nat-id"
                 help="Twice NAT ID"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 optional="true"
                 >
                 <PARAM
                     name="twice-nat-id-value"
                     help="Twice NAT ID value"
                     ptype="UINT"
                     >
                 </PARAM>
             </PARAM>
         </PARAM>


         <PARAM
             name="natPortType"
             help=""
             ptype="PORT_TYPE"
             >
            <PARAM
                 name="global-ip"
                 help="Global IP"
                 ptype="IP_ADDR"
                 >
             </PARAM>

             <PARAM
                 name="global-port"
                 help="Global Port"
                 ptype="UINT"
                 >
             </PARAM>

             <PARAM
                 name="local-ip"
                 help="Local IP"
                 ptype="IP_ADDR"
                 >
             </PARAM>

             <PARAM
                 name="local-port"
                 help="Local Port"
                 ptype="UINT"
                 >
             </PARAM>

             <PARAM
                 name="natType"
                 help=""
                 ptype="TYPE"
                 optional="true"
                 >
             </PARAM>

             <PARAM
                 name="twice-nat-id"
                 help="Twice NAT ID"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 optional="true"
                 >
                 <PARAM
                     name="twice-nat-id-value"
                     help="Twice NAT ID value"
                     ptype="UINT"
                     >
                 </PARAM>
             </PARAM>
         </PARAM>
     </PARAM>
     <ACTION builtin="clish_pyobj">sonic_cli_nat patch_nat_napt_mapping_table global-ip=${global-ip} port-type=${natPortType} global-port=${global-port} local-ip=${local-ip} local-port=${local-port} type=${natType} twice-nat-id=${twice-nat-id-value}</ACTION>
    </COMMAND>

     <COMMAND
         name="no static"
         help="Remove static NAT entry"
         mode="subcommand"
         ptype="SUBCOMMAND"
         >
         <PARAM
             name="no-static-subcommands"
             help="Remove static NAT subcommands"
             mode="switch"
             ptype="SUBCOMMAND"
             >
             <PARAM
                 name="all"
                 help="Remove all static configurations"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
             </PARAM>
             <PARAM
                 name="basic"
                 help="Static NAT"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >

                 <PARAM
                     name="global-ip"
                     help="Global IP"
                     ptype="IP_ADDR"
                 >
                 </PARAM>

             </PARAM>
             
             <PARAM
                 name="natPortType"
                 help=""
                 ptype="PORT_TYPE"
                 >
                <PARAM
                     name="global-ip"
                     help="Global IP"
                     ptype="IP_ADDR"
                     >
                 </PARAM>

                 <PARAM
                     name="global-port"
                     help="Global Port"
                     ptype="UINT"
                     >
                 </PARAM>

             </PARAM>
         </PARAM>

   <ACTION builtin="clish_pyobj">sonic_cli_nat delete_nat_napt_mapping_table global-ip=${global-ip} port-type=${natPortType} global-port=${global-port}</ACTION>

     </COMMAND>

   </VIEW>


     <VIEW name="enable-view">

     <!--====================clear <translations | statistics>======================-->

    <COMMAND
        name="clear nat"
        help="Clear NAT">
        <PARAM
            name="clear-subcommands"
            help="Clear subcommands"
            mode="switch"
            ptype="SUBCOMMAND"
            >
            <PARAM
                name="translations"
                help="Clearing dynamic NAT translations"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
            </PARAM>
            <PARAM
                name="statistics"
                help="Clearing NAT statistics"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
            </PARAM>
        </PARAM>
    <ACTION builtin="clish_pyobj">sonic_cli_nat rpc_nat_clear ${clear-subcommands}</ACTION>
    </COMMAND>


    <!--show nat -->
    <COMMAND
         name="show nat"
         help="Show NAT info"
         >
         <PARAM
             name="nat-subcommands"
             help="NAT subcommands"
             mode="switch"
             ptype="SUBCOMMAND"
             >
             <!-- show nat translations -->
             <PARAM
                 name="translations"
                 help="NAT translations"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
                 <PARAM
                     name="count"
                     help="NAT translations count"
                     ptype="SUBCOMMAND"
                     mode="subcommand"
                     optional="true"
                     >
                 </PARAM>
             </PARAM>

             <!-- show nat statistics -->
             <PARAM
                 name="statistics"
                 help="NAT Statistics"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
             </PARAM>

              <!-- show nat config -->
             <PARAM
                 name="config"
                 help="NAT Configurations"
                 ptype="SUBCOMMAND"
                 mode="subcommand"
                 >
                 <PARAM
                     name="config-subcommands"
                     help="NAT Config subcommands"
                     mode="switch"
                     ptype="SUBCOMMAND"
                     optional="true"
                     >
                     <PARAM
                         name="static"
                         help="NAT Static config"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                     </PARAM>
                     <PARAM
                         name="pool"
                         help="NAT pool config"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                     </PARAM>
                     <PARAM
                         name="bindings"
                         help="NAT static bindings"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                     </PARAM>
                     <PARAM
                         name="globalvalues"
                         help="NAT static globals"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                     </PARAM>
                     <PARAM
                         name="zones"
                         help="NAT zones"
                         ptype="SUBCOMMAND"
                         mode="subcommand"
                         >
                     </PARAM>
                 </PARAM>
             </PARAM>
         </PARAM>
          <ACTION builtin="clish_pyobj">sonic_cli_nat show_nat_commands nat-sub=${nat-subcommands} config-sub=${config-subcommands} count=${count} </ACTION>
     </COMMAND>

   </VIEW>
</CLISH_MODULE>

