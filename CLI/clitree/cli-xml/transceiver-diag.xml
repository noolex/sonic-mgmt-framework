<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema     http://www.dellemc.com/sonic/XMLSchema/clish.xsd">
  <VIEW name="configure-view">
    <COMMAND name="interface transceiver" help="Transceiver Configuration"/>
    <COMMAND name="interface transceiver diagnostics" help="Transceiver diagnostics configuration"
             command_tables="sonic-transceiver:sonic-transceiver/TRANSCEIVER_DIAG/TRANSCEIVER_DIAG_LIST/ifname={ifname}"
             render_command_cb="xcvr_diag_ctrl">
      <PARAM name="ifcfg-xcvr-diag-subcommands"
             help="Configure transceiver diagnostics subcommands"
             mode="switch"
             ptype="SUBCOMMAND">
        <PARAM name="loopback"  help="Configure transceiver diagnostics loopback" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-lb-subcommands"
                 help="Configure transceiver diagnostics loopback subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="host-side-input" help="Host/System side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="host-side-output" help="Host/System side output" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-input" help="Media/Line side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-output" help="Media/Line side output" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
        <PARAM name="pattern"  help="Configure transceiver diagnostics PRBS pattern generation and checking" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-prbs-subcommands"
                 help="Configure transceiver diagnostics PRBS pattern generation and checking subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="checker-host" help="Enable PRBS pattern checking at the system/host side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="checker-media" help="Enable PRBS pattern checking at the media/line side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="generator-host" help="Enable PRBS pattern generation at the system/host side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="generator-media" help="Enable PRBS pattern generation at the media/line side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
      </PARAM>
      <PARAM name="phy-if-name" help="Interface" ptype="PHY_INTERFACE" optional="true"></PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_xcvr_diag ${__full_line} </ACTION>
    </COMMAND>
    <COMMAND name="no interface transceiver" help="Transceiver configuration"/>
    <COMMAND name="no interface transceiver diagnostics" help="Transceiver diagnostics configuration">
      <PARAM name="ifcfg-xcvr-diag-subcommands"
             help="Configure transceiver diagnostics subcommands"
             mode="switch"
             ptype="SUBCOMMAND">
        <PARAM name="loopback"  help="Configure transceiver diagnostics loopback" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-lb-subcommands"
                 help="Configure transceiver diagnostics loopback subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="host-side-input" help="Host/System side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="host-side-output" help="Host/System side output" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-input" help="Media/Line side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-output" help="Media/Line side output" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
        <PARAM name="pattern"  help="Configure transceiver diagnostics PRBS pattern generation and checking" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-prbs-subcommands"
                 help="Configure transceiver diagnostics PRBS pattern generation and checking subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="checker-host" help="Enable PRBS pattern checking at the system/host side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="checker-media" help="Enable PRBS pattern checking at the media/line side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="generator-host" help="Enable PRBS pattern generation at the system/host side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="generator-media" help="Enable PRBS pattern generation at the media/line side" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
      </PARAM>
      <PARAM name="phy-if-name" help="Interface" ptype="PHY_INTERFACE" optional="true"></PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_xcvr_diag ${__full_line} </ACTION>
    </COMMAND>
  </VIEW>
</CLISH_MODULE>

