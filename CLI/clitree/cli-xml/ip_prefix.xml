<!DOCTYPE CLISH_MODULE [
]>

<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema
                        http://www.dellemc.com/sonic/XMLSchema/clish.xsd">

   <VIEW name="enable-view">
    <COMMAND name="show ip prefix-list" help="IPv4 prefix list">
        <PARAM name="list-name" help="Name of prefix-list" ptype="STRING_63" optional="true"> </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ip_prefix_show_specific show_ip_prefix.j2 ${list-name}</ACTION>
    </COMMAND>
    <COMMAND name="show ipv6 prefix-list" help="IPv6 prefix list">
        <PARAM name="list-name" help="Name of prefix-list" ptype="STRING_63" optional="true"> </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ipv6_prefix_show_specific show_ipv6_prefix.j2 ${list-name}</ACTION>
    </COMMAND>
   </VIEW>

   <VIEW
       name="configure-view"
       >
    <!-- interface commands -->
    <COMMAND
        name="ip prefix-list"
        help="Build a prefix list"
        command_keys="name=*,prefix=*,mask=*"
        command_tables="sonic-routing-policy-sets:sonic-routing-policy-sets/PREFIX/PREFIX_LIST/set_name={name},ip_prefix={prefix},masklength_range={mask};sonic-routing-policy-sets:sonic-routing-policy-sets/PREFIX_SET/PREFIX_SET_LIST/name={name}"
        render_command_cb="v4prefix_lists_cmd"
        >
        <PARAM
            name="prefix-name"
            help="Name of prefix-list (Max: 63 characters)"
            ptype="STRING_63"
            >
            <PARAM
                name="ip-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="permit"
                    help="Allow action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Deny action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ip_prefix_create ${__full_line}</ACTION>
    </COMMAND>

    <COMMAND
        name="ipv6 prefix-list"
        help="Build a prefix list"
        command_keys="name=*,prefix=*,mask=*"
        command_tables="sonic-routing-policy-sets:sonic-routing-policy-sets/PREFIX/PREFIX_LIST/set_name={name},ip_prefix={prefix},masklength_range={mask};sonic-routing-policy-sets:sonic-routing-policy-sets/PREFIX_SET/PREFIX_SET_LIST/name={name}"
        render_command_cb="v6prefix_lists_cmd"
        >
        <PARAM
            name="prefix-name"
            help="Name of prefix-list (Max: 63 characters)"
            ptype="STRING_63"
            >
            <PARAM
                name="ipv6-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="permit"
                    help="Allow action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Deny action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ip_prefix_create ${__full_line}</ACTION>
    </COMMAND>
    <COMMAND
        name="no ip prefix-list"
        help="Delete a prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of prefix-list"
            ptype="STRING_63"
            >
            <PARAM
                name="ip-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                optional="true"
                >
                <PARAM
                    name="permit"
                    help="Allow action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Deny action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ip_prefix_delete ${__full_line}</ACTION>
    </COMMAND>

    <COMMAND
        name="no ipv6 prefix-list"
        help="Delete IPv6 prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of prefix-list"
            ptype="STRING_63"
            >
            <PARAM
                name="ipv6-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                optional="true"
                >
                <PARAM
                    name="permit"
                    help="Allow action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Deny action for the prefixes"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="UINT"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="UINT"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION builtin="clish_pyobj">sonic_cli_ip_prefix ip_prefix_delete ${__full_line}</ACTION>
    </COMMAND>
</VIEW>
</CLISH_MODULE>

