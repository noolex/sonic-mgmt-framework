<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema     http://www.dellemc.com/sonic/XMLSchema/clish.xsd">
  <VIEW name="enable-view">
    <COMMAND name="show interface transceiver-diagnostics" help="Show transceiver diagnostics info">
      <PARAM name="if-xcvr-diag-subcommands"
             help="Interface transceiver diagnostics subcommands"
             mode="switch"
             ptype="SUBCOMMAND">
        <PARAM name="loopback"  help="Show transceiver diagnostics loopback capability" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="if-xcvr-diag-lb-subcommands"
                 help="Interface transceiver diagnostics loopback subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="capability" help="Show transceiver diagnostics loopback capability" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="controls" help="Show transceiver diagnostics loopback controls" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
        <PARAM name="params" help="Show transceiver operating parameters" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
      </PARAM>
      <PARAM name="phy-if-name" help="Interface" ptype="PHY_INTERFACE" optional="true"></PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_xcvr ${__full_line} </ACTION>
    </COMMAND>
  </VIEW>
  <VIEW name="configure-view">
    <COMMAND name="interface diagnostics" help="Configure transceiver diagnostics">
      <PARAM name="ifcfg-xcvr-diag-subcommands"
             help="Configure transceiver diagnostics subcommands"
             mode="switch"
             ptype="SUBCOMMAND">
        <PARAM name="loopback"  help="Configure transceiver diagnostics loopback" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-lb-subcommands"
                 help="Configure transceiver diagnostics loopback subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="host-side-input" help="Host/System side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-input" help="Media/Line side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
      </PARAM>
      <PARAM name="phy-if-name" help="Interface" ptype="PHY_INTERFACE" optional="true"></PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_xcvr ${__full_line} </ACTION>
    </COMMAND>
    <COMMAND name="no interface diagnostics" help="Configure transceiver diagnostics">
      <PARAM name="ifcfg-xcvr-diag-subcommands"
             help="Configure transceiver diagnostics subcommands"
             mode="switch"
             ptype="SUBCOMMAND">
        <PARAM name="loopback"  help="Configure transceiver diagnostics loopback" mode="subcommand" ptype="SUBCOMMAND">
          <PARAM name="ifcfg-xcvr-diag-lb-subcommands"
                 help="Configure transceiver diagnostics loopback subcommands"
                 mode="switch"
                 ptype="SUBCOMMAND">
            <PARAM name="host-side-input" help="Host/System side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
            <PARAM name="media-side-input" help="Media/Line side input" mode="subcommand" ptype="SUBCOMMAND"></PARAM>
          </PARAM>
        </PARAM>
      </PARAM>
      <PARAM name="phy-if-name" help="Interface" ptype="PHY_INTERFACE" optional="true"></PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_xcvr ${__full_line} </ACTION>
    </COMMAND>
  </VIEW>
</CLISH_MODULE>
