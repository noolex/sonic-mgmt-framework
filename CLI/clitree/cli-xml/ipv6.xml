<!--=======================================================-->
<!--                Config Interface PHY-MODE              -->
<!--=======================================================-->

<VIEW name="configure-if-view">
    <COMMAND
        name="ipv6"
        help="Interface Internet Protocol v6 config commands"
    />
    <COMMAND
        name="no ipv6"
        help="Interface Internet Protocol v6 config commands"
    />
    <COMMAND
        name="ipv6 address"
        help="IPv6 address"
        command_tables="sonic-interface:sonic-interface/INTERFACE/INTERFACE_IPADDR_LIST/portname={name},ip_prefix={prfx}"
        render_command_cb="ipv6_eth_ip_address"
        >
        <PARAM
            name="addr"
            help="IPv6 address with mask"
            ptype="IPV6_ADDR_MASK" />
            <ACTION builtin="clish_pyobj">sonic_cli_if patch_openconfig_if_ip_interfaces_interface_subinterfaces_subinterface_ipv6_addresses_address_config ${iface} ${addr}</ACTION>
    </COMMAND>
    <COMMAND
        name="no ipv6 address"
        help="Delete IPv6 address(es) on the interface" >
        <PARAM
            name="addr"
            help="IPv6 address"
            optional="true"
            ptype="IPV6_ADDR_MASK" />
        <ACTION builtin="clish_pyobj">sonic_cli_if delete_phy_if_ip ${iface} ${addr} False</ACTION>

    </COMMAND>

    <MACRO name="DHCPV6-RELAY-CONFIG-OPTIONS" arg="{iface},sonic-interface:sonic-interface/INTERFACE/INTERFACE_LIST/portname={name},ipv6_eth_dhcp_relay,sonic-interface:sonic-interface/INTERFACE/INTERFACE_LIST"> </MACRO>
    <MACRO name="INTF_IPV6_ENABLE" arg="${iface},sonic-interface:sonic-interface/INTERFACE/INTERFACE_LIST/portname={name},sonic-interface:sonic-interface/INTERFACE/INTERFACE_LIST/ipv6_use_link_local_only=enable"> </MACRO>
    <MACRO name="DHCPV6-RELAY-NO-CONFIG-OPTIONS" arg="{iface} "> </MACRO>

    <COMMAND
        name="ipv6 neighbor"
        help="Configure static ND"
        command_tables="sonic-ip-neighbor:sonic-ip-neighbor/NEIGH/NEIGH_LIST/staticintf={name},staticip={addr}"
        dbpath="sonic-ip-neighbor:sonic-ip-neighbor/NEIGH/NEIGH_LIST/family=IPv6"
         >
        <PARAM
            name="static-ip"
            help="IP Address"
            dbpath="sonic-ip-neighbor:sonic-ip-neighbor/NEIGH/NEIGH_LIST/staticip"
            ptype="IPV6_ADDR" />
        <PARAM
            name="neigh"
            help="MAC Address"
            ptype="MAC_ADDR"
            dbpath="sonic-ip-neighbor:sonic-ip-neighbor/NEIGH/NEIGH_LIST/neigh"
            />

        <ACTION builtin="clish_pyobj">sonic_cli_if patch_openconfig_if_ipv6_interfaces_interface_subinterfaces_subinterface_static_nd_config ${iface} ${static-ip}
             ${neigh}</ACTION>
        <DOCGEN>
            <DESCRIPTION>
                Configure static ND.
            </DESCRIPTION>
            <USAGE>
                Use this command to configure static ND.
            </USAGE>
            <EXAMPLE summary="Configure static ARP">
                sonic-cli(config)# interface Ethernet 0
                sonic-cli(conf-if-Ethernet0)# ip neighbor ip_addr mac
            </EXAMPLE>
        </DOCGEN>
    </COMMAND>

    <COMMAND
        name="no ipv6 neighbor"
        help="Unconfigure static ND" >
        <PARAM
            name="static-ip"
            help="IP Address"
            ptype="IPV6_ADDR" />
        <PARAM
            name="neigh"
            help="MAC Address"
            ptype="MAC_ADDR" />

        <ACTION builtin="clish_pyobj">sonic_cli_if delete_openconfig_if_ipv6_interfaces_interface_subinterfaces_subinterface_static_nd_config ${iface} ${static-ip}
             ${neigh}</ACTION>
        <DOCGEN>
            <DESCRIPTION>
                Unconfigure static ND.
            </DESCRIPTION>
            <USAGE>
                Use this command to unconfigure static ND.
            </USAGE>
            <EXAMPLE summary="Unconfigure static ND">
                sonic-cli(config)# interface Ethernet 0
                sonic-cli(conf-if-Ethernet0)# no ipv6 neighbor ip_addr mac
            </EXAMPLE>
        </DOCGEN>
    </COMMAND>
  </VIEW>

