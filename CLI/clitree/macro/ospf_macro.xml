<ROOT>

<MACRODEF name="BIND-INTF-TO-OSPF">

    <COMMAND name = "ip ospf" help = "Configures OSPFv2 interface mode parameters" command_keys="name=*,address=*" command_tables="sonic-ospfv2:sonic-ospfv2/OSPFV2_INTERFACE/OSPFV2_INTERFACE_LIST/name={name},address={address}" render_command_cb='interface_ip_ospf'>
    </COMMAND>

    <COMMAND name = "no ip ospf" help = "Unconfigures OSPFv2 interface mode parameters" >
    </COMMAND>

    <COMMAND name = "no ip ospf interface" help = "Unconfigures all OSPFv2 parameters in the interfaces" >
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_unconfig ${iface} ${vlan_name} ${po_name} ${lo_name} &#xA;</ACTION>
    </COMMAND>


    <COMMAND name="ip ospf area" help="Configures OSPFv2 area identifier" >
      <PARAM name = "area-id" help = "OSPFv2 area ID" ptype = "AREA_NUM_DOT">
         <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true" />
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_area_id ${iface} ${vlan_name} ${po_name} ${lo_name} ${area-id} ${ip-address}  &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf area" help="Unconfigures OSPFv2 area identifier" >
         <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true" />
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_area_id ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="ip ospf authentication" help="Enable OSPFv2 authentication" >
        <PARAM name = "authtype" help = "" ptype = "SUBCOMMAND" mode="switch" optional="true">
          <PARAM name="message-digest" help="Message digest authentication type" ptype="SUBCOMMAND" mode="subcommand" >
             <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
          </PARAM>
          <PARAM name="null" help="Use null authentication" ptype="SUBCOMMAND" mode="subcommand"> 
             <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
          </PARAM>
        </PARAM>    
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" /> 
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_type ${iface} ${vlan_name} ${po_name} ${lo_name} ${authtype} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf authentication" help="Disable OSPFv2 authentication" >
        <PARAM name = "authtype" help = "" ptype = "SUBCOMMAND" mode="switch" optional="true">
          <PARAM name="message-digest" help="Message-digest authentication type" ptype="SUBCOMMAND" mode="subcommand" >
             <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
          </PARAM>
          <PARAM name="null" help="Use null authentication" ptype="SUBCOMMAND" mode="subcommand">
             <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
          </PARAM>
        </PARAM>
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_type ${iface} ${vlan_name} ${po_name} ${lo_name} ${authtype} ${ip-address} &#xA;</ACTION>
    </COMMAND>
  
    <COMMAND name="ip ospf authentication-key" help="Configures OSPFv2 plain text authentication type password" escape_chars="">
      <PARAM name="authkey" help="Authentication password" ptype="STRING">
        <PARAM name="encrypted"  help = "Password is in encrypted format" ptype="SUBCOMMAND" mode="subcommand" optional="true">
            <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
        </PARAM>
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_key ${iface} ${vlan_name} ${po_name} ${lo_name} ${authkey} ${encrypted} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf authentication-key" help="Unconfigures OSPFv2 plain text authentication type password" >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_key ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>
  
    <COMMAND name="ip ospf bfd" help="Configures OSPFv2 interface BFD" >
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_bfd ${iface} ${vlan_name} ${po_name} ${lo_name} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf bfd" help="Unconfigures OSPFv2 interface BFD" >
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_bfd ${iface} ${vlan_name} ${po_name} ${lo_name} &#xA;</ACTION>
    </COMMAND>
 
    <COMMAND name="ip ospf cost" help="Configures OSPFv2 interface cost" >
      <PARAM name="interface-cost" help="Cost value" ptype="RANGE_1_65535" >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_cost ${iface} ${vlan_name} ${po_name} ${lo_name} ${interface-cost} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf cost" help="Unconfigures OSPFv2 interface cost" >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_cost ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>


    <COMMAND name="ip ospf dead-interval" help="Configures OSPFv2 adjacency dead interval" >
      <PARAM name = "deadtype" help = "" ptype = "SUBCOMMAND" mode="switch" >
        <PARAM name="deadinterval" help="Time in seconds" ptype="RANGE_1_65535" optional="true">
          <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
        </PARAM>
        <PARAM name="minimal" help="Minimal 1s dead-interval with fast sub-second hellos" ptype="SUBCOMMAND" mode="subcommand" optional="true" >
          <PARAM name="hello-multiplier" help="Hello multiplier factor" ptype="SUBCOMMAND" mode="subcommand" optional="true" >
            <PARAM name="hellomultiplier" help="Number of Hellos to send each second" ptype="RANGE_1_10" >
              <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true" />
            </PARAM>
          </PARAM>
        </PARAM>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_dead_interval ${iface} ${vlan_name} ${po_name} ${lo_name} ${deadtype} ${deadinterval} ${hellomultiplier} ${ip-address} &#xA;</ACTION>
    </COMMAND>


    <COMMAND name="no ip ospf dead-interval" help="Unconfigures OSPFv2 adjacency dead interval" >
      <PARAM name="deadtype" help = "" ptype = "SUBCOMMAND" mode="switch" optional="true">
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
        <PARAM name="minimal" help="Minimal 1s dead-interval with fast sub-second hellos" ptype="SUBCOMMAND" mode="subcommand" optional="true" >
          <PARAM name="hello-multiplier" help="Hello multiplier factor" ptype="SUBCOMMAND" mode="subcommand" optional="true" >
              <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true"/>
          </PARAM>
        </PARAM>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_dead_interval ${iface} ${vlan_name} ${po_name} ${lo_name} ${deadtype} ${ip-address} &#xA;</ACTION>
    </COMMAND>
    <COMMAND name="ip ospf hello-interval" help="Configures OSPFv2 neighbour hello interval" >
      <PARAM name="hellointerval" help="Time in seconds" ptype="RANGE_1_65535" optional="true">
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_hello_interval ${iface} ${vlan_name} ${po_name} ${lo_name} ${hellointerval} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf hello-interval" help="Unconfigures OSPFv2 neighbour hello interval" >
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_hello_interval ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>

    </COMMAND>

    <COMMAND name="ip ospf message-digest-key" help="Configures OSPFv2 message digest key and password" escape_chars="" command_keys="name=*,address=*,authentication-key-id=*" command_tables="sonic-ospfv2:sonic-ospfv2/OSPFV2_INTERFACE_MD_AUTHENTICATION/OSPFV2_INTERFACE_MD_AUTHENTICATION_LIST/name={name},address={address},authentication-key-id={authentication-key-id}" render_command_cb='interface_ip_ospf_md_auth'>
      <PARAM name="keyid" help="Key ID" ptype="RANGE_1_255" >
        <PARAM name="md5" help="Uses MD5 algorithm" ptype="SUBCOMMAND" mode="subcommand" >
          <PARAM name = "md5key" help = "Message digest password" ptype = "STRING" >
            <PARAM name="encrypted"  help = "Password is in encrypted format" ptype="SUBCOMMAND" mode="subcommand" optional="true">
              <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
            </PARAM>
            <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
          </PARAM>
        </PARAM>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_key_id ${iface} ${vlan_name} ${po_name} ${lo_name} ${keyid} ${md5key} ${encrypted} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf message-digest-key" help="Unconfigures OSPFv2 message digest key and password" >
      <PARAM name="keyid" help="Key ID" ptype="RANGE_1_255" >
        <PARAM name="md5" help="Uses MD5 algorithm" ptype="SUBCOMMAND" mode="subcommand" >
            <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
        </PARAM>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_authentication_key_id ${iface} ${vlan_name} ${po_name} ${lo_name} ${keyid} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>


    <COMMAND name="ip ospf mtu-ignore" help="Disables OSPFv2 MTU mismatch detection" >
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true" />
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_mtu_ignore ${iface} ${vlan_name} ${po_name} ${lo_name} ${ip-address}  &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf mtu-ignore" help="Enables OSPFv2 MTU mismatch detection" >
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_mtu_ignore ${iface} ${vlan_name} ${po_name} ${lo_name} ${ip-address}  &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="ip ospf network" help="Configures OSPFv2 interface network type" >
      <PARAM name = "networktype" help = "" ptype = "SUBCOMMAND" mode="switch" >
        <PARAM name = "broadcast" help = "Broadcast multi-access link network" ptype = "SUBCOMMAND" mode="subcommand" />
        <PARAM name = "point-to-point" help = "Point-to-point link network" ptype = "SUBCOMMAND" mode="subcommand" />
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_network_type ${iface} ${vlan_name} ${po_name} ${lo_name} ${networktype}  &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf network" help="Unconfigures OSPFv2 interface network type" >
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_network_type ${iface} ${vlan_name} ${po_name} ${lo_name} ${networktype}  &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="ip ospf priority" help="Configures OSPFv2 adjacency router priority">
      <PARAM name = "priorityval" help = "Priority value" ptype = "RANGE_0_255" >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_priority ${iface} ${vlan_name} ${po_name} ${lo_name} ${priorityval} ${ip-address} &#xA;</ACTION>
    </COMMAND>
  
    <COMMAND name="no ip ospf priority" help="Unconfigures OSPFv2 adjacency router priority">
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_priority ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="ip ospf retransmit-interval" help="Configures OSPFv2 LSA retransmit interval" >
      <PARAM name = "retransmitinterval" help = "Time value in seconds" ptype = "RANGE_2_65535"  >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_retransmit_interval ${iface} ${vlan_name} ${po_name} ${lo_name} ${retransmitinterval} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf retransmit-interval" help="Unconfigures OSPFv2 LSA retransmit interval" >
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR" optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_retransmit_interval ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>
 
    <COMMAND name="ip ospf transmit-delay" help="Configures OSPFv2 LSA transmit delay interval" >
      <PARAM name = "transmitdelay" help = "Time in seconds" ptype = "RANGE_1_65535" >
        <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true"/>
      </PARAM>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 patch_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_transmit_delay ${iface} ${vlan_name} ${po_name} ${lo_name} ${transmitdelay} ${ip-address} &#xA;</ACTION>
    </COMMAND>

    <COMMAND name="no ip ospf transmit-delay" help="Unconfigures OSPFv2 LSA transmit delay interval" >
      <PARAM name = "ip-address" help = "Interface IPv4 address" ptype = "IP_ADDR"  optional="true"/>
      <ACTION builtin="clish_pyobj"> sonic_cli_ospfv2 delete_openconfig_interfaces_interface_subinterfaces_subinterface_ip_ospf_config_transmit_delay ${iface} ${vlan_name} ${po_name} ${lo_name} 0 ${ip-address} &#xA;</ACTION>
    </COMMAND>

</MACRODEF>

</ROOT>
