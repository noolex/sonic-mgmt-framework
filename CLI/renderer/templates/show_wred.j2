{% if json_output -%}
{% for key_json in json_output %}
    {% if key_json == "openconfig-qos-ext:wred-profiles" %}
        {% set wred_profs = json_output[key_json]["wred-profile"] %}
    {% else %}
      {% if key_json == "openconfig-qos-ext:wred-profile" %}
        {% set wred_profs = json_output[key_json] %}
