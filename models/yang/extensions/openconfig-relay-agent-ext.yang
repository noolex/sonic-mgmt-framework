module openconfig-relay-agent-ext {

    yang-version "1";

    namespace "http://openconfig.net/yang/extensions/oc-relay-agent-ext";
    prefix "oc-relay-ext";

/*
    revision "2020-03-11" {
        description
        "Initial release";
         reference "0.1";
